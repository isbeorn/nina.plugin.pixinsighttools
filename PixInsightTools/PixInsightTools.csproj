  &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools&quot;&#xD;&#xA;    mkdir  &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts&quot;&#xD;&#xA;)&#xD;&#xA;echo &quot;Copying $(PlatformName) $(TargetFileName)&quot;&#xD;&#xA;copy &quot;$(TargetDir)$(TargetFileName)&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\$(TargetFileName)&quot; /Y&#xD;&#xA;&#xD;&#xA;copy &quot;$(TargetDir)Scripts\startup.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\startup.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\calibrate.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\calibrate.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\alignment.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\alignment.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\pixelmath_stack.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\pixelmath_stack.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\resample.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\resample.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\postprocess_stack.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\postprocess_stack.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\colorcombine_stack.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\colorcombine_stack.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\flat_integration.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\flat_integration.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\debayer.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\debayer.js&quot; /Y&#xD;&#xA;copy &quot;$(TargetDir)Scripts\noise_evaluation.js&quot; &quot;%25localappdata%25\NINA\Plugins\PixInsight Tools\Scripts\noise_evaluation.js&quot; /Y" />
  </Target>
</Project>